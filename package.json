{
  "name": "react-switch-case-statement",
  "version": "1.2.0",
  "description": "This package provides React components that emulate switch-case behavior for conditional rendering in React applications.",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/react-switch-case-statement.umd.js",
  "module": "./dist/react-switch-case-statement.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-switch-case-statement.es.js",
      "require": "./dist/react-switch-case-statement.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "keywords": [
    "react",
    "component",
    "npm",
    "react-switch-case",
    "switch-case",
    "switch"
  ],
  "author": "Supriya Kundu",
  "license": "MIT"
}
